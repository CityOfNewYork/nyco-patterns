{
  "config": {
    "style_src": "src/scss/site.scss",
    "style_bundle": "bundle/styles/site.concat.css",
    "style_dist": "dist/styles/site.min.css"
  },
  "name": "nyco-patterns",
  "version": "0.0.1",
  "description": "Agency patterns for the Mayor's Office for Economic Opportunity",
  "main": "index.js",
  "scripts": {
    "test": "",
    "start": "node index.js",
    "styles-watch": "nodemon --ext scss --watch src/scss -x 'npm run styles'",
    "styles-postcss": "cssnano --sourcemap < $npm_package_config_style_bundle > $npm_package_config_style_dist",
    "styles": "node-sass --sourceMapEmbed true --include-path node_modules $npm_package_config_style_src $npm_package_config_style_bundle && npm run styles-postcss",
    "scripts-webpack": "webpack --define process.env.NODE_ENV='\"production\"' --config webpack.config.js",
    "scripts-watch": "nodemon --ext js --watch src/js -x 'npm run scripts'",
    "scripts": "npm run scripts-webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfNewYork/nyco-patterns.git"
  },
  "author": "Mayor's Office for Economic Opportunity",
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/CityOfNewYork/nyco-patterns/issues"
  },
  "homepage": "https://github.com/CityOfNewYork/nyco-patterns#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cssnano-cli": "^1.0.5",
    "d3": "^4.9.1",
    "d3-selection-multi": "^1.0.1",
    "express": "^4.15.3",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "slm": "^1.0.0",
    "uglify-js": "^3.0.15",
    "webpack": "^2.6.1"
  },
  "dependencies": {}
}
