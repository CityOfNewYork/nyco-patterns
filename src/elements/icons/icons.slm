
- for key in this.variables.icons

  - href = key
  - for i in this.variables.iconVariants
    - if href.indexOf(this.variables.iconVariants[i]) > 0
      - href = href.replace(this.variables.iconVariants[i], '')

  div class='flex flex-col justify-between items-center border border-secondary-grey-30t p-2 mb-4'
    div class='w-full'

      div class='text-font-size-small'

        h3 class='text-h6 capitalize mt-0 mb-2' ${key.replace(/-/g," ")}

        div class='py-4 text-center bg-secondary-white px-2 mb-2 flex items-center justify-center' style='min-height:150px'
          svg class='${key} icon-xlarge text-color-blue-dark'
            use xlink:href='#${key.split("--")[0]}'

        p class='mb-0 flex justify-between'
          | xlink:href
          code #${key}