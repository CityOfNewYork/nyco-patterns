= mixin('nav');
  nav class='screen-tablet:pb-2'
    a class='${class_nav_link} pr-2' href='index'
      | Home
    a class='${class_nav_link}' href='${link_source}'
      | Repository
      span class='${class_nav_link_sub}'
        = this.site.versions.package;
    a class='${class_nav_link} mb-2' href='${link_nyc_opportunity}' target='_blank' rel='noindex nofollow'
      | NYC Opportunity
    a class='btn' data-theme='www1.nyc.gov' onclick='toggleTheme(event.target.dataset.theme)'
      | NYC.gov Styles

  nav class='screen-tablet:pb-2'
    span class='${class_nav_link_header}' Elements:
    a class='${class_nav_link}' href='buttons' Buttons
    a class='${class_nav_link}' href='colors' Colors
    a class='${class_nav_link}' href='icons' Icons
    a class='${class_nav_link}' href='layouts' Layouts
    a class='${class_nav_link}' href='tables' Tables

  nav class='screen-tablet:pb-2'
    span class='${class_nav_link_header}' Objects:
    a class='${class_nav_link}' href='feed' Feed

  nav class='screen-tablet:pb-2'
    span class='${class_nav_link_header}' Utilities:
    a class='${class_nav_link}' href='type' Type

  nav class='screen-tablet:pb-2'
    span class='screen-tablet:block pb-1' Dependencies:
    a class='${class_nav_link}' href='${link_city_style_guide}' target='_blank' rel='noindex nofollow'
      | NYC.gov Style Guide
    a class='${class_nav_link}' href='${link_bootstrap}' target='_blank' rel='noindex nofollow'
      | Bootstrap
      span class='${class_nav_link_sub}'
        = this.site.versions.bootstrap;
    a class='${class_nav_link}' href='${link_tailwind_docs}' target='_blank' rel='noindex nofollow'
      | Tailwind.css
      span class='${class_nav_link_sub}'
        = this.site.versions.tailwindcss;


